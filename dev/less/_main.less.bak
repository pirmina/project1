div {box-sizing:content-box;}
.ie7 #contents .main-content .flex_wrap {margin:-1px;}
.ie7 #contents .main-content .flex_wrap .flex_box {width:585px;border:1px solid #eaeaea;}
.ie7 #contents .main .full_bg_wrap:first-child {margin-top:-50px;margin-bottom:-50px;}
.ie7 .main .main-content .swiper-wrapper {border:1px solid #eaeaea;}
.ie7 #contents .format-content, .ie8 #contents .format-content {position:relative;zoom:1;}
.ie7 #contents .format-content, .ie8 #contents .format-content:after {display:block;content:"";clear:both;}
.ie7 #contents .format-content .swiper-slide-con .main-detail-area,
.ie8 #contents .format-content .swiper-slide-con .main-detail-area{width:384px;}
.top-banner {margin:-50px 0;height:610px;background:#111314;}
.top-banner .swiper-slide {text-align:center;}
.top-banner .swiper-slide a, .top-banner .swiper-slide a img {display:block;margin:0 auto;}
.top-banner.swiper-container .swiper-pagination {left:50%;bottom:50px;display:inline-block;margin-left:-723px;max-width:1446px;text-align:left;}
.top-banner .swiper-pagination .swiper-pagination-bullet {margin-right:18px;width:9px;height:9px;background:transparent;border:1px solid #fff;border-radius:50%;}
.top-banner .swiper-pagination .swiper-pagination-bullet:first-child {margin-left:130px;}
.top-banner .swiper-pagination .swiper-pagination-bullet-active {background:#fff;}
.main .top-banner .btn-slide-wrap {display:none;top:50%;margin-top:-40px;width:100%;}
.main .top-banner .btn-slide-wrap .swiper-button-prev {border-right:none;}
.main h3 {padding-bottom:30px;height:24px;text-align:center;}
.main h3 span {display:block;}
.main h3 span.sp_ico {margin:0 auto;}
.main .cont_wrap .swiper-container {/* overflow-y:hidden; */margin-top:-56px;padding-top:56px;}
.main .btn-slide-wrap, .contact-about .btn-slide-wrap {position:absolute;right:0;top:2px;width:71px;height:36px;}
.main .cont_wrap .swiper-container .swiper-slide {float:left;}
.main .btn-slide-wrap .swiper-button-prev, .contact-about .btn-slide-wrap .swiper-button-prev {left:0;top:0;border-right:1px solid #ddd;}
.main .btn-slide-wrap .swiper-button-next, .contact-about .btn-slide-wrap .swiper-button-next {right:0;top:0;}
.main .btn-slide-wrap .swiper-button-disabled {opacity:1;}
.main .main-content .swiper-wrapper {position:relative;overflow:hidden;max-width:1245px;height:519px;box-sizing:inherit;background:#fff;border-left:1px solid #eaeaea;zoom:1;}
.main .main-content .swiper-wrapper:after {display:block;content:"";clear:both;}
#contents .main-content .swiper-wrapper .swiper-slide {/*position:relative; float:left;height:517px;box-sizing:content-box;zoom:1;border-left:1px solid #eaeaea;border-top:1px solid #eaeaea;border-bottom:1px solid #eaeaea; */}
.main-content .swiper-wrapper .swiper-slide .swiper-slide-con {position:relative;float:left;margin:-1px 0 0 -1px;padding:18px;/* width:585px;height:222px; */box-sizing:border-box;background:#fff;/* border:1px solid #eaeaea; */zoom:1;}
.main-content .swiper-wrapper .swiper-slide .swiper-slide-con:after {display:block;content:"";clear:both;}
/* .main-content .swiper-wrapper .swiper-slide .swiper-slide-con:first-child {margin:0;border:none;} */
.main-content .swiper-slide-con .main-thumbnail {float:left;display:block;width:395px;}
.main-content .swiper-slide-con .main-detail-area {float:right;padding:21px 0 0;width:172px;}
.main-content .swiper-slide-con .main-detail-area h4 a {font-size:18px;line-height:21px;color:#333;}
.main-content .swiper-slide-con .main-detail-area h4 span {display:block;}
.main-content .swiper-slide-con .main-detail-area .year-ep {display:block;margin-top:27px;font-size:14px;line-height:16px;color:#252525;}
.main-content .swiper-slide-con .main-detail-area .abridge-desc {font-size:14px;line-height:17px;color:#989898;}
.main-content .swiper-slide-con .main-detail-area .abridge-desc:before {display:block;content:"";margin:11px 0 11px 1px;width:20px;height:1px;background:#b2b2b2;}
.main-content .swiper-pagination {display:none;}
.format-content .swiper-wrapper {overflow-x:hidden;position:relative;margin:0 auto;width:916px;height:260px;zoom:1;border:none;}
.format-content .flex_wrap .flex_box:nth-child(odd):before {border:none;}
.format-content .swiper-wrapper:after {display:block;content:"";clear:both;}
.format-content .swiper-slide-con {position:relative;width:100%;zoom:1;}
.format-content .swiper-slide-con:after {display:block;content:"";clear:both;}
.swiper-container.format-content .main-thumbnail {float:left;padding:18px;width:395px;box-sizing:content-box;background:#fff;border:1px solid #eaeaea;}
.format-content .main-thumbnail img {display:block;width:395px;}
.format-content .main-detail-area {float:left;padding:38px 32px 0 50px;width:400px;}
.format-content h4 a, .format-content h4 span {display:block;font-size:18px;line-height:23px;color:#1a1a1a;}
.format-content .year-ep {font-size:14px;line-height:19px;color:#333;}
.format-content .abridge-desc {overflow-y:hidden;height:143px;font-size:14px;line-height:18px;color:#666;}
.format-content .abridge-desc:before {display:block;content:"";margin:15px 0;width:26px;height:1px;background:#1a1a1a;}
.format-content .btn-slide-wrap {top:132px;width:100%;height:59px;}
.format-content .btn-slide-wrap div {position:absolute;width:31px;height:61px;}
.format-content .btn-slide-wrap span {position:absolute;display:block;width:31px;height:61px;}
.format-content .btn-slide-wrap .swiper-button-pre {left:50px;}
.format-content .btn-slide-wrap .swiper-button-next {right:50px;}
.format-content .swiper-pagination {display:none;}
.contact-about {position:relative;zoom:1;}
.contact-about:after {display:block;content:"";clear:both;}
.contact-about h3 {text-align:left;}
.main .contact-about h3 span.sp_ico {margin:0;}
.contact-about .main-contact {position:relative;float:left;width:748px;}
.contact-about .main-about {float:right;width:466px;}
.main .contact-about .swiper-container {margin:0;padding:0;border-top:1px solid #eaeaea;}
.contact-about .contact-about-con {position:relative;width:745px;border-left:1px solid #eaeaea;border-right:1px solid #eaeaea;}
.contact-about .contact-about-con .swiper-wrapper {overflow:hidden;width:745px;height:336px;zoom:1;}
.contact-about .contact-about-con .swiper-wrapper:after {display:block;content:"";clear:both;}
.contact-about .contact-about-con .swiper-wrapper .profile_pic {float:none;margin:0 auto;}
.contact-about .main-about .about-con-jtbc {padding:14px;border:1px solid #eaeaea}
.contact-about .swiper-wrapper .swiper-slide {float:left;padding:40px 0;width:247px;height:254px;border-right:1px solid #eaeaea;border-bottom:1px solid #eaeaea;}
.contact-about .swiper-wrapper .swiper-slide:first-child {}
.contact-about img, .contact-about p, .contact-about a {margin:0 auto;text-align:center;}
/* .contact-about .contact-thumb {display:block;} */
.contact-about .contact-name {margin-top:18px;font-size:18px;line-height:23px;color:#333;text-align:center;font-weight:bold;}
.contact-about .content-position {font-size:14px;line-height:20px;color:#666;}
.contact-about .contact-mail {display:block;margin-top:15px;width:99px;font-size:14px;line-height:27px;color:#333;font-weight:bold;border:1px solid #bfbfbf;}
.main-content .swiper-wrapper {background:transparent;}
@media all and (max-width:1245px){
	div {box-sizing:border-box;}
	.main .flex_wrap .flex_box:before {border:none;}
	#contents .cont_wrap {max-width:100%;}
	#contents div.full_bg_wrap:first-child {padding:0;}
	.top-banner {margin:0;height:auto;}
	.m-main .top-banner img {width:100%;}
	#contents .main-content .swiper-wrapper {/* padding:0 2.81%; */max-width:100%;height:auto;background:none;}
	.m-main .top-banner .swiper-slide a, .m-main .top-banner .swiper-slide a img {width:100%;}
	.main-content .swiper-slide-con .main-thumbnail {width:100%;}
	.main-content .swiper-slide-con .main-detail-area {padding-left:1.58%;padding-bottom:20px;width:100%;}
	.m-main .top-banner .btn-slide-wrap {display:block;}
	#contents .main-content .btn-slide-wrap {display:none;}
	#contents .main-content .swiper-wrapper .swiper-slide .swiper-slide-con {margin:0;padding:0 1.56%;width:50%;height:auto;background:transparent;border:none;}
	#contents .main-content .swiper-wrapper .swiper-slide {width:100%;height:auto;border:none;}
	.main-content .abridge-desc {display:none;}
	.format-content .abridge-desc {display:block;height:36px;}
	.main-content .swiper-slide-con .main-detail-area h4 {font-weight:bold;}
	.main-content .swiper-slide-con .main-detail-area h4 a span {font-size:14px;line-height:17px;}
	.main-content .swiper-slide-con .main-detail-area .year-ep {display:block;margin:0;padding:2px 0;font-size:12px;line-height:15px;}
	#contents .main-content .swiper-pagination {display:block;bottom:0;}
	#contents .swiper-pagination span.swiper-pagination-bullet-active {background:#191919;}
	#contents .swiper-pagination .swiper-pagination-bullet {background:none;border:1px solid #191919;}
	.format-content {padding-bottom:61px;}
	.format-content .swiper-slide {display:block;/* padding:0 4.37%; */min-width:320px;width:100%;}
	.format-content .swiper-slide-con {display:table;}
	.format-content .swiper-slide .main-thumbnail,
	.format-content .swiper-slide .main-detail-area {float:none;padding:0;width:44.76%;vertical-align:top;}
	.format-content .swiper-slide-con .main-thumbnail {display:table-cell;background:none;border:none;}
	.format-content .swiper-slide-con .main-detail-area {display:table-cell;padding-left:10.48%;}
	.format-content .abridge-desc:before {display:none;}
	.format-content .btn-slide-wrap {display:none;}
	.format-content .swiper-slide .swiper-slide-con {width:100%;}
	.format-content .swiper-slide .main-thumbnail img {width:100%;}
	.format-content .swiper-slide .main-detail-area {}
	.format-content .swiper-wrapper {margin:0 auto;width:91.23%;height:auto;}
	.format-content.swiper-container .swiper-pagination {display:block;bottom:-32px;height:61px;}
	#contents .m-main .contact-about {margin-bottom:-55px;}
	.contact-about h3 {text-align:center;}
	.m-main .contact-about h3 span.sp_ico {margin:0 auto;}
	.contact-about .main-contact, .contact-about .main-about {float:none;}
	.contact-about .main-contact {padding-bottom:38px;width:auto;}
	.contact-about .main-contact .swiper-container {}
	.contact-about .main-contact .btn-slide-wrap {display:none;}
	.contact-about .main-contact .contact-about-con {width:100%;}
	.contact-about .main-about {padding-top:22px;padding-bottom:44px;width:100%;background:#f2f2f2;}
	.main-about .about-con-jtbc {margin:0 auto;width:91.25%;background:#fff;}
	.main-about .about-con-jtbc img {width:100%;}
	.main-about .profile_pic {display:block;}
}
/*  */